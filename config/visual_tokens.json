{
  "schema": "visual-tokens@1",
  "grid": {
    "unit": 4,
    "padding": {
      "outer": 12,
      "inner": 8
    }
  },
  "typography": {
    "scale": {
      "big": 26,
      "label": 12,
      "small": 10,
      "time": 10
    },
    "weights": {
      "bold": "bold",
      "normal": "normal"
    }
  },
  "icons": {
    "sizes": {
      "md": 24,
      "sm": 16
    },
    "stroke_width": 2
  },
  "spacing": {
    "section_gap": 4,
    "line_height_multiplier": 1.2
  }
}


